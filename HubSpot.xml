        &hapikey=@(Model.AccessToken)
        &count=@(Model.PageCursor.NextTake)
        @(Model.PageCursor.Page != 0 && Model.HasMore == "true" ? "&vidOffset=" + Model.VidOffset : "")
        ]]>
        </Fetch.Url>
      </Fetch>
