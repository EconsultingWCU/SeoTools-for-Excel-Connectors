	<RestConnector Id="ProjectSummaryStatistics" Title="Project Summary Statistics" HelpUrl="https://seranking.com/api-project-management.html#anchor7" HelpText="Obtain a project’s summary statistics.">
    <Parameters>
      <Text Id="SiteId" Title="Site Id" Debug.DefaultValue="549058" Required="true"  Select.Connector="ProjectSites" Select.IdField="Id" Select.TitleField="Name" HelpText=""/>
