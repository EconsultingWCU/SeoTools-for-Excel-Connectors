      <JsonPath Expr="lighthouseResult.audits.html-lang-valid.score" Id="Html-Lang-Valid" Converter="Double" HelpText="Specifying a valid [BCP 47 language]"/>
      <JsonPath Expr="lighthouseResult.audits.aria-roles.score" Id="Aria-Roles" Converter="Double" HelpText="ARIA roles must have valid values in order to perform their intended accessibility functions."/>
      <JsonPath Expr="lighthouseResult.audits.aria-required-attr.score" Id="Aria-Required-Attr" Converter="Double" HelpText="Some ARIA roles have required attributes that describe the state of the element to screen readers."/>
      <JsonPath Expr="lighthouseResult.audits.aria-valid-attr.score" Id="Aria-Valid-Attr" Converter="Double" HelpText="Assistive technologies, like screen readers, can't interpret ARIA attributes with invalid names."/>
      <JsonPath Expr="lighthouseResult.audits.aria-valid-attr-value.score" Id="Aria-Valid-Attr-Value" Converter="Double" HelpText="Assistive technologies, like screen readers, can't interpret ARIA attributes with invalid values."/>
      <JsonPath Expr="lighthouseResult.audits.aria-required-parent.score" Id="Aria-Required-Parent" Converter="Double" HelpText="Some ARIA child roles must be contained by specific parent roles to properly perform their intended accessibility functions."/>
      <JsonPath Expr="lighthouseResult.audits.aria-required-children.score" Id="Aria-Required-Children" Converter="Double" HelpText="Some ARIA parent roles must contain specific child roles to perform their intended accessibility functions."/>
    </Parse>
		<Resource Id="Fail"/>
  </RestConnector>

