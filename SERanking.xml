      </HttpSettings>
      <Fetch.Url>
        <![CDATA[
        https://api4.seranking.com/sites/@(Model.SiteId)/stat
        ]]>
      </Fetch.Url>
    </Fetch>
    <Parse>
			<JsonPath Expr="site_id" Id="Id" Converter="Long" HelpText="Unique website ID"/>
