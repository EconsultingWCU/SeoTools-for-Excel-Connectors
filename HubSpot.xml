        <JsonPath Expr="properties.amount.value" Id="Amount" Title="Amount" Tag="amount" Converter="Double" HelpText="The amount of the deal." DefaultValue="0"/>
        <JsonPath Expr="properties.dealstage.value" Id="Stage" Title="Stage" Tag="dealstage" Converter="String" DefaultValue=""/>
        <JsonPath Expr="properties.num_associated_contacts.value" Id="Contacts" Title="Contacts" Tag="num_associated_contacts" Converter="Int" HelpText="Number of associated contacts." DefaultValue=""/>
        <JsonPath Expr="properties.notes.value" Id="Notes" Title="Notes" Tag="notes" Converter="Int" DefaultValue=""/>
        <JsonPath Expr="properties.num_contacted_notes.value" Id="ContactedNotes" Title="Contacted Notes" Tag="num_contacted_notes" Converter="Int" DefaultValue=""/>
        <JsonPath Expr="properties.pipeline.value" Id="Pipeline" Title="Pipeline" Tag="pipeline" Converter="String" DefaultValue=""/>
        <JsonPath Expr="properties.dealtype.value" Id="Type" Title="Type" Tag="dealtype" Converter="String" DefaultValue=""/>
        <JsonPath Expr="properties.description.value" Id="Description" Title="Description" Tag="description" Converter="String" DefaultValue=""/>
        <Compute Expr="@(DateTimeOffset.FromUnixTimeMilliseconds(Model.Inp))" Id="CloseDate" Title="Close Date" Converter="DateTime" Tag="closedate" DefaultValue="">
          <JsonPath Expr="properties.closedate.value" Id="Inp" Converter="Long"/>
        </Compute>
        <Compute Expr="@(DateTimeOffset.FromUnixTimeMilliseconds(Model.Inp))" Id="LastModified" Title="Last Modified" Converter="DateTime" Tag="hs_lastmodifieddate" HelpText="The Timestamp when deal was last modified" DefaultValue="">
          <JsonPath Expr="properties.hs_lastmodifieddate.value" Id="Inp" Converter="Long"/>
        </Compute>
        <Compute Expr="@(DateTimeOffset.FromUnixTimeMilliseconds(Model.Inp))" Id="Created" Title="Created" Converter="DateTime" Tag="createdate" HelpText="The Timestamp when deal was created" DefaultValue="">
          <JsonPath Expr="properties.createdate.value" Id="Inp" Converter="Long"/>
        </Compute>
        <JsonPath Expr="properties.hubspot_owner_id.value" Id="OwnerId" Title="Owner Id" Tag="hubspot_owner_id" Converter="Int" DefaultValue=""/>
      </JsonPath>
    </Parse>
    <Resource Id="Fail"/>
  </RestConnector>

  <RestConnector Id="DealsAssociated" Title="Deals Associated With" Group="Deals" HelpText="Returns all of the deals associated with a contact or company.">
    <Parameters>
      <Select Id="AssociatedType" Title="Associated With" DefaultValue="contact" Required="false">
        <DataSource>
          <Item Id="contact" Title="Contact"/>
          <Item Id="company" Title="Company"/>
        </DataSource>
      </Select>
      <Text Id="ObjectId" Title="Contact/Company Id" Debug.DefaultValue="204727" Required="true"/>
    </Parameters>
    <Paging PageSize="100">
      <Parse>
        <JsonPath Id="Offset" Expr="offset"/>
        <JsonPath Id="HasMore" Expr="hasMore"/>
      </Parse>
    </Paging>
    <Fetch>
      <Fetch.Url>
        <![CDATA[
        https://api.hubapi.com/deals/v1/deal/associated/@(Model.AssociatedType)/@(Model.ObjectId)
        /paged?properties=@SelectedPropertiesFields()
        &hapikey=@(Model.AccessToken)
        &count=@(Model.PageCursor.NextTake)
        @(Model.PageCursor.Page != 0 && Model.HasMore == "true" ? "&vidOffset=" + Model.VidOffset : "")
        ]]>
        </Fetch.Url>
      </Fetch>
    <Parse>
      <Table DefaultConverter="auto">
        <JsonPath Id="Headers" Expr="keys($.deals[0].properties)">
					<Compute Id="Headers" Title="Headers">
						<Compute.Expr>
							<![CDATA[
							@{
								string Cleaned = Regex.Replace(Model.Input, "_", " ");
								string Cleaned2 = Utils.Capitalize(Cleaned);
							}
							@Cleaned2
							]]>
						</Compute.Expr>
						<JsonPath Expr="key" Id="Input" DefaultValue=""/>
					</Compute>
        </JsonPath>
        <JsonPath Id="Cells" Expr="keys(deals[*].properties[*])">
          <JsonPath Expr="value" />
        </JsonPath>
      </Table>
    </Parse>
    <Resource Id="Fail"/>
  </RestConnector>

