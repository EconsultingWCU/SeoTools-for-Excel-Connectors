<?xml version="1.0" encoding="utf-8" ?>
<Suite Category="SEO" Title="_MultiSelect" Id="_MultiSelect" RequireVersion="8.0" SourceUrl="https://github.com/nielsbosma/SeoTools-for-Excel-Connectors/blob/master/_MultiSelect.xml" HelpUrl="http://seotoolsforexcel.com/_multiselect/">

  <Author Name="Dovydas Meilunas" Url="https://github.com/dovydasm"/>

  <Settings HelpText="What's this?" HelpUrl="http://seotoolsforexcel.com/_multiselect/">
    <Text Id="ApiKey" Title="API Key" Required="true" HelpUrl="http://seotoolsforexcel.com/_multiselect/"/>
  </Settings>

  <Resources>
    <Resource Id="Dimensions">
      <Item Id="ym:s:visits" Title="Sessions" Group="Basic Metrics" HelpText="Total number of visits."/>
      <Item Id="ym:s:avgVisitDurationSeconds" Group="Behavior" Title="Time on site" HelpText="Average session duration in minutes and seconds."/>
      <Item Id="ungrouped1" Title="Ungrouped 1 Title" HelpText="Ungrouped1 help"/>
      <Item Id="ungrouped2" Title="Ungrouped 2 Title" HelpText="Ungrouped2 help"/>
    </Resource>
  </Resources>

  <RestConnector Id="Test" Title="Test" HelpUrl="https://www.bing.com/" HelpText="Barebones connector to test multiselect">
    <Parameters>
      <MultiSelect Id="Dimensions" Name="Dimensions" Required="true" Separator="," Max="100">
        <DataSource>
          <Resource Id="Dimensions"/>
        </DataSource>
      </MultiSelect>
    </Parameters>
    <Fetch>
      <Fetch.Url>
        <![CDATA[
        https://www.bing.com/search
        ]]>
      </Fetch.Url>
    </Fetch>
    <Parse>
      <JsonPath Id="$.zzzzz"/>
    </Parse>
  </RestConnector>
</Suite>