  <RestConnector Id="TrendingbyCountry" Title="Trending by Country" Hidden="false">
    <Parameters>
      <Select Id="Country" Title="Country" Required="true" DefaultValue="1" HelpText="Extracts the most trending keywords of the specific country.">
        <DataSource>
          <Resource Id="Countries"/>
        </DataSource>
      </Select>
    </Parameters>
    <Fetch>
      <Fetch.Url>
        <![CDATA[
        http://hawttrends.appspot.com/api/terms/
        ]]>
      </Fetch.Url>
    </Fetch>
    <Parse>
      <JsonPath Expr="$.@(Model.Country)[*]">
        <JsonPath Expr="." Id="US" Title="US" Converter="String" HelpText=""/>
      </JsonPath>
    </Parse>
  </RestConnector>

