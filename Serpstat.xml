  <RestConnector Id="KeywordAutocompletes" Title="Keyword Search Suggestions" Group="Keyword Reports" HelpText="These report lists autocomplete suggestions for the keyword you requested (they are found by the full-text search)." HelpUrl="https://serpstat.com/api/33-keyword-search-suggestions-suggestions/">
    <Parameters>
      <Text Id="Keyword" Title="Keyword" Debug.DefaultValue="seo tools" Required="true"/>
      <Resource Id="SelectEngine"/>
    </Parameters>
    <Fetch>
      <Fetch.Url>
        <![CDATA[
        http://api.serpstat.com/v3/suggestions?token=@Model.Key
        &query=@Utils.UrlEncode(Model.Keyword)
