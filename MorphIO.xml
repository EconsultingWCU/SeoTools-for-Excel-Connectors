<?xml version="1.0" encoding="utf-8" ?>
<Suite Title="Morhp.io" Id="Morhp.io" RequireVersion="6.1.2" SourceUrl="https://github.com/nielsbosma/SeoTools-for-Excel-Connectors/blob/master/Morhpio.xml" HelpUrl="http://seotoolsforexcel.com/Morhpio/" HelpText="Documentation">

  <Author Name="Victor Sandberg" Url="http://community.seotoolsforexcel.com/users/diskborste/activity"/>

  <Settings HelpText="What's this?" HelpUrl="http://seotoolsforexcel.com/Morhpio/">
    <Text Id="ApiKey" Title="API Key" Required="true" HelpUrl="http://seotoolsforexcel.com/Morhpio/"/>
  </Settings>	

  <RestConnector Id="ExtractData" Title="Extract Data" HelpUrl="https://morph.io/documentation/api">
	<Fetch>
    <Fetch.Url>
      <![CDATA[
			https://api.morph.io/everypolitician-scrapers/twitter-list-sweden/data.json?key=2IQ8Dr95GZsoDdcW%2FZ6h&query=select%20*%20from%20%27data%27%20limit%2010
      ]]>
      </Fetch.Url>
    </Fetch>
    <Parse>
      <JsonPath Expr="$.*">
        <JsonPath Expr="id" Id="Id" Title="Id"/>
        <JsonPath Expr="name" Id="Name" Title="Name"/>
      </JsonPath>
    </Parse>
    <Fail>
      <JsonPath Expr="message"/>
    </Fail>
  </RestConnector>

</Suite>
