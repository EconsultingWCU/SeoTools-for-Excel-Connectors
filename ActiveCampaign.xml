  <RestConnector Id="UserLookup" Title="User Lookup" HelpText="Retrieve an existing user by looking up their email address" HelpUrl="https://developers.activecampaign.com/reference#get-user-email">
    <Parameters>
      <Select Id="InputType" Title="Input Type" Required="false" DefaultValue="email">
        <DataSource>
          <Resource Id="Inputs"/>
        </DataSource>
      </Select>
      <Text Id="Input" Title="Input" Debug.DefaultValue="" Required="true"/>
		</Parameters>
    <Fetch>
			<Resource Id="Header"/>
      <Fetch.Url>
        <![CDATA[
				https://@(Model.BaseUrl).api-us1.com/api/3/users@(Model.InputType)@(Model.Input)
