	<RestConnector Id="KeywordSearchVolume" Title="Keyword Search Volume" HelpUrl="https://seranking.com/api-total-information.html#anchor5" HelpText="Get the search volume data for a specified region and keyword list.">
    <Parameters>
      <Text Id="RegionId" Title="Region Id" Debug.DefaultValue="182" Required="true" Select.Connector="Regions" Select.IdField="Id" Select.TitleField="Name" HelpText=""/>
      <Text Id="Ids" Title="List of Keywords" Debug.DefaultValue="seo" Required="true" Multiline="true" HelpText="Keywords separated by newline."/>
    </Parameters>
    <Batch Parameter="Ids" Separator="\n" ItemsPerBatch="10"/>
    <Fetch>
      <HttpSettings>
        <RequestHeaders>
          <Header Name='Authorization'>Token @Model.ApiKey</Header>
        </RequestHeaders>
      </HttpSettings>
      <Fetch.Url>
        <![CDATA[
        https://api4.seranking.com/system/volume
				?region_id=@(Model.RegionId)
				&keyword[]=@Fetchkeywords()
        ]]>
      </Fetch.Url>
    </Fetch>
    <Parse>
      <JsonPath Expr="keys(.)">
        <JsonPath Expr="key" Id="Keyword" Converter="String"/>
        <JsonPath Expr="value" Id="Volume" Converter="Long" HelpText="Keyword search volume"/>
      </JsonPath>
		</Parse>
    <Fail>
      <JsonPath Expr="message"/>
