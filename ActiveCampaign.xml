				<JsonPath Expr="firstName" Id="FirstName" Converter="String"/>
				<JsonPath Expr="lastName" Id="LastName" Converter="String"/>
				<JsonPath Expr="udate" Id="uDate" Converter="Auto" DefaultValue=""/>
			</JsonPath>
    </Parse>
  </RestConnector>

</Suite>