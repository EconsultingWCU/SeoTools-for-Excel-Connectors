	<RestConnector Id="ProjectKeywordsOverview" Title="Project Keywords Overview" HelpUrl="https://seranking.com/api-project-management.html#anchor8" HelpText="Obtain general statistics on a project’s keywords.">
    <Parameters>
      <Text Id="SiteId" Title="Site Id" Debug.DefaultValue="549058" Required="true"  Select.Connector="ProjectSites" Select.IdField="Id" Select.TitleField="Name" HelpText=""/>
      <Text Id="EngineId" Title="Site Engine Id" Debug.DefaultValue="" Required="true"  Select.Connector="ProjectSearchEngines" Select.IdField="SiteEngineId" Select.TitleField="LangCode" HelpText=""/>
    </Parameters>
    <Fetch>
      <HttpSettings>
        <RequestHeaders>
          <Header Name='Authorization'>Token @Model.ApiKey</Header>
        </RequestHeaders>
      </HttpSettings>
      <Fetch.Url>
        <![CDATA[
        https://api4.seranking.com/sites/@(Model.SiteId)/positions
				?site_engine_id=@(Model.EngineId)
				&with_serp_features=1
        ]]>
      </Fetch.Url>
    </Fetch>
    <Parse>
      <JsonPath Expr="$.[0].keywords.*">
        <JsonPath Expr="id" Id="Id" Converter="Long" HelpText="Unique query ID"/>
        <JsonPath Expr="name" Id="Name" Converter="String" HelpText="Query Name"/>
        <JsonPath Expr="volume" Id="Volume" Converter="Long" HelpText="Search volume"/>
        <JsonPath Expr="competition" Id="Competition" Converter="Long" HelpText="Competition"/>
        <JsonPath Expr="suggested_bid" Id="SuggestedBid" Converter="Long" HelpText="Cost per click"/>
        <JsonPath Expr="kei" Id="Kei" Converter="Long" HelpText="Keyword Efficiency Index"/>
        <JsonPath Expr="results" Id="Results" Converter="Long" HelpText="Number of results for a given keyword in Google"/>
        <JsonPath Expr="features.tads" Id="GoogleTads" Title="Google Tads" Converter="Bool" DefaultValue="0" HelpText=""/>
        <JsonPath Expr="features.knowledge_graph" Id="GoogleKnowledgeGraph" Title="Google Knowledge Graph" Converter="Bool" DefaultValue="0" HelpText=""/>
        <JsonPath Expr="features.featured_snippets" Id="GoogleFeaturedSnippets" Title="Google Featured Snippets" Converter="Bool" DefaultValue="0" HelpText=""/>
        <JsonPath Expr="features.top_stories" Id="GoogleTopStories" Title="Google Top Stories" Converter="Bool" DefaultValue="0" HelpText=""/>
        <JsonPath Expr="features.sitelinks" Id="GoogleSitelinks" Title="Google Sitelinks" Converter="Bool" DefaultValue="0" HelpText=""/>
        <JsonPath Expr="features.images" Id="GoogleImages" Title="Google Images" Converter="Bool" DefaultValue="0" HelpText=""/>
        <JsonPath Expr="features.video" Id="GoogleVideo" Title="Google Video" Converter="Bool" DefaultValue="0" HelpText=""/>
        <JsonPath Expr="features.reviews" Id="GoogleReviews" Title="Google Reviews" Converter="Bool" DefaultValue="0" HelpText=""/>
			</JsonPath>
    </Parse>
    <Fail>
      <JsonPath Expr="message"/>
    </Fail>
  </RestConnector>

	<RestConnector Id="ProjectKeywordTrend" Title="Project Keyword Trend" HelpUrl="https://seranking.com/api-project-management.html#anchor8" HelpText="Obtain the statistics on a project’s keyword ranking check for a specified time period.">
    <Parameters>
      <Text Id="SiteId" Title="Site Id" Debug.DefaultValue="549058" Required="true"  Select.Connector="ProjectSites" Select.IdField="Id" Select.TitleField="Name" HelpText=""/>
      <Text Id="EngineId" Title="Site Engine Id" Debug.DefaultValue="" Required="true"  Select.Connector="ProjectSearchEngines" Select.IdField="SiteEngineId" Select.TitleField="LangCode" HelpText=""/>
      <Text Id="KeywordId" Title="Keyword Id" Debug.DefaultValue="" Required="true"  Select.Connector="ProjectKeywords" Select.IdField="Id" Select.TitleField="Keyword" HelpText=""/>
      <DateInterval Id="DateInterval" Title="Date Interval" Required="false"/>
    </Parameters>
    <Fetch>
      <HttpSettings>
        <RequestHeaders>
          <Header Name='Authorization'>Token @Model.ApiKey</Header>
        </RequestHeaders>
      </HttpSettings>
      <Fetch.Url>
        <![CDATA[
        https://api4.seranking.com/sites/@(Model.SiteId)/positions
				?site_engine_id=@(Model.EngineId)
        @if(Model.DateInterval.StartDate != null && Model.DateInterval.EndDate != null)
        {
          @: &date_from=@(((DateTimeOffset)Model.DateInterval.StartDate).ToString("yyyy-MM-dd"))
          @: &date_to=@(((DateTimeOffset)Model.DateInterval.EndDate).ToString("yyyy-MM-dd"))
        }
        ]]>
      </Fetch.Url>
    </Fetch>
    <Parse>
      <JsonPath Expr="$.[0].keywords[?(@@.id=='@(Model.KeywordId)')].positions.*">
        <JsonPath Expr="date" Id="Date" Converter="DateTime" HelpText=""/>
        <JsonPath Expr="pos" Id="Position" Converter="Long" HelpText="Current position"/>
        <JsonPath Expr="change" Id="Change" Converter="Long" HelpText="Change in position compared to previous date (could be negative)"/>
        <JsonPath Expr="price" Id="Price" Converter="Double" Checked="false" HelpText="Price calculated from financial report settings"/>
        <JsonPath Expr="paid_position" Id="PaidPosition" Converter="Long" Checked="false" HelpText="Position in paid Google SERPs"/>
        <JsonPath Expr="is_map" Id="IsMap" Converter="Bool" Checked="false" HelpText="Indicates where the position was found"/>
        <JsonPath Expr="map_position" Id="MapPosition" Converter="Long" Checked="false" HelpText="Position in the maps block with the “Display organic and maps search results separately” option enabled"/>
			</JsonPath>
    </Parse>
    <Fail>
      <JsonPath Expr="message"/>
    </Fail>
  </RestConnector>

