  <RestConnector Id="KeywordsRelated" Title="Keywords Related" Group="Keyword Reports" HelpText="This report gives you a comprehensive list of related keywords whose SERP is similar to the one the requested keyword has." HelpUrl="https://serpstat.com/api/35-related-keywords-relatedkeywords/">
    <Parameters>
      <Text Id="Keyword" Title="Keyword" Debug.DefaultValue="seo tools" Required="true"/>
      <Number Id="ConnectionStrength" Title="Minimum Connection Strength (1-20)" HelpText="Criteria for the degree of keywords similarity. Serpstat finds related keywords according to the similarity of their top-20 search results."/>
      <Resource Id="SelectEngine"/>
    </Parameters>
    <Paging PageSize="100"/>
    <Fetch>
      <Fetch.Url>
        <![CDATA[
        http://api.serpstat.com/v3/related_keywords?token=@Model.Key
        &query=@Utils.UrlEncode(Model.Keyword)
        @if(Model.ConnectionStrength != 0)
        {
          @: &weight=@Utils.UrlEncode(Model.ConnectionStrength)
        }
        &se=@Model.Engine
        &page_size=@(Model.PageCursor.NextTake)
        &page=@(Model.PageCursor.Page)
        ]]>
      </Fetch.Url>
    </Fetch>
    <Parse>
      <JsonPath Expr="result.hits.*">
        <JsonPath Expr="keyword" Id="Keyword" Title="Keyword" Converter="String"/>
        <JsonPath Expr="concurrency" Id="Concurrency" Title="Concurrency" Converter="Int"/>
        <JsonPath Expr="cost" Id="Cost" Title="Cost" Converter="Double"/>
        <JsonPath Expr="types" Id="Types" Title="Types" Converter="String"/>
        <JsonPath Expr="weight" Id="ConnectionStrength" Title="Connection Strength" Converter="Int"/>
      </JsonPath>
    </Parse>
  </RestConnector>

  <RestConnector Id="KeywordTopSearchResults" Title="Keyword Top Search Results" Group="Keyword Reports" HelpText="This report shows you Google top 100 organic search results for the keyword you requested." HelpUrl="https://serpstat.com/api/37-top-for-a-keyword-keywordtop/">
